  "scripts": {
    "test": "NODE_ENV=test DB_NAME=sticks_test npx sequelize-cli db:create && NODE_ENV=test DB_NAME=sticks_test npx sequelize-cli db:migrate && DB_NAME=sticks_test jest --detectOpenHandles --forceExit tests/*.ts  && NODE_ENV=test DB_NAME=sticks_test npx sequelize-cli db:drop",
    "build": "webpack",
    "start": "sequelize-cli db:migrate && nodemon --watch . --ext ts --exec ts-node --delay 3 index.ts",
    "debug": "nodemon --watch . --ext ts --exec 'node --inspect ./node_modules/.bin/ts-node' index.ts",
    "lint": "npx eslint . --ext .ts,.tsx",
    "lint-fix": "npx eslint --fix . --ext .ts,.tsx",
    "migrate": "npx sequelize-cli db:migrate"
